{
  "name": "neepsend",
  "version": "0.0.1",
  "scripts": {
    "prepare": "husky install",
    "lint": "eslint --ext js,ts,tsx src/ webpack/",
    "prettier": "prettier \"**/*.ts{,x}\" --check",
    "pre-commit": "npm run lint && npm run prettier",
    "pre-push": "npm run build; npm run deploy",
    "dev": "webpack serve --config ./webpack/config.dev.ts",
    "build": "rm -rf build; webpack --progress --config ./webpack/config.prod.ts",
    "deploy": "gh-pages -d build"
  },
  "devDependencies": {
    "@types/node": "^16.18.9",
    "@types/ramda": "^0.29.5",
    "@types/react": "^18.2.24",
    "@types/react-dom": "^18.2.8",
    "@typescript-eslint/eslint-plugin": "^6.7.3",
    "@typescript-eslint/parser": "^6.7.3",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.8.1",
    "css-minimizer-webpack-plugin": "^5.0.1",
    "eslint": "^8.50.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "gh-pages": "^6.0.0",
    "html-webpack-plugin": "^5.5.3",
    "husky": "^8.0.3",
    "mini-css-extract-plugin": "^2.7.6",
    "postcss": "^8.4.20",
    "postcss-loader": "^7.0.2",
    "prettier": "^3.0.3",
    "style-loader": "^3.3.3",
    "tailwindcss": "^3.2.4",
    "ts-loader": "^9.4.4",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1",
    "webpack-merge": "^5.9.0"
  },
  "dependencies": {
    "ramda": "^0.29.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.16.0",
    "zod": "^3.22.2"
  }
}
